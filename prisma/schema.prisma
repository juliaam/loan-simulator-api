generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model loan {
  id          Int     @id @default(autoincrement())
  cpf         String  @db.Char(11)
  uf          String  @db.Char(2)
  birth       String  @db.VarChar
  total_value BigInt?
  month_value BigInt?
}
